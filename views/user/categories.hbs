<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Category Navigation</title>
    <style>
        /* Add some basic styling for better visualization */
        .category {
            cursor: pointer;
            margin-bottom: 10px;
            color: blue;
            text-decoration: underline;
        }

        .subcategory {
            display: none;
            margin-left: 20px;
        }
    </style>
    <script>
        function toggleSubcategories(categoryId) {
            // Get all subcategories with the specified category ID
            var subcategories = document.querySelectorAll('.subcategory[data-category="' + categoryId + '"]');

            // Toggle the display of subcategories
            subcategories.forEach(function (subcategory) {
                subcategory.style.display = subcategory.style.display === 'none' ? 'block' : 'none';
            });
        }
    </script>
</head>
<body>

