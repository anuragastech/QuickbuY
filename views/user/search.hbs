<body>
    <h1 style="margin-top:8%;">Product Search</h1>
    <form id="searchForm">
        <label for="searchQuery">Search:</label>
        <input type="text" id="searchQuery" name="searchQuery" placeholder="Enter your search query" required>
        <button type="submit">Search</button>
    </form>

    <div id="searchResults"></div>

 <div class="container mt-5">
        <h1>Search Results</h1>
        <div class="row">
            {{#each products}}
                <div class="col-md-4 mb-4">
                    <div class="card">
                        <img src="{{this.image.url}}" class="card-img-top" alt="{{this.productname}}">
                        <div class="card-body">
                            <h5 class="card-title">{{this.productname}}</h5>
                            <p class="card-text">{{this.brand}}</p>
                        </div>
                    </div>
                </div>
            {{/each}}
        </div>
        {{#unless products.length}}
            <p>No results found.</p>
        {{/unless}}
    </div>
    <!-- Include Bootstrap JS (optional) -->
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
