            <div class="am">
  <div class="am">
    {{#if products}}
        {{#each products}}
            <div class="product {{getProductStyle @index}}">
                <p>Product Name: {{this.productname}}</p>
                <p>Manufacturer: {{this.manufacturename}}</p>
                <p>Brand: {{this.brand}}</p>
                <p>Price: {{this.price}}</p>
                <p>Description: {{this.description}}</p>
                <p>Category: {{this.category.title}}</p>
                <p>Subcategory: {{this.subcategory.title}}</p>
                <img src="{{this.image.url}}" alt="{{this.productname}} Image" style="max-width: 100px; height: auto;">
            </div>
        {{/each}}
    {{else}}
        <p>No products found.</p>
    {{/if}}
</div>

<script>
    Handlebars.registerHelper('getProductStyle', function(index) {
        // You can implement a logic here to determine the style based on the index
        // For example, alternate between two styles
        return index % 2 === 0 ? 'even-product' : 'odd-product';
    });
</script>


