      <script>
        function getCookie(name) {
          const match = document.cookie.match(new RegExp('(^| )' + name + '=([^;]+)'));
          if (match) return match[2];
        }

        const alertMessage = getCookie('alert');

        if (alertMessage) {
          Toastify({
            text: alertMessage,
            duration: 3000, // 3 seconds
            close: true,
            gravity: 'top',
            position: 'center',
            backgroundColor: 'linear-gradient(to right, #00b09b, #96c93d)',
          }).showToast();
          // Remove the alert cookie
          document.cookie = 'alert=; expires=Thu, 01 Jan 1970 00:00:00 GMT; path=/';
        }
      </script>
