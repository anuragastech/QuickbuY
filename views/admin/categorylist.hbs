      <script>
    $(document).ready(function () {
      // Event listener for edit buttons
      $('.editBtn').click(function () {
        // Get the closest table row to the clicked button
        var row = $(this).closest('tr');

        // Extract category data from the table row
        var categoryId = $(this).val(); // Get the category ID from the button's value attribute
        var title = row.find('td:eq(1)').text(); // Assuming title is in the first column
        var description = row.find('td:eq(2)').text(); // Assuming description is in the second column
        var imageUrl = row.find('td:eq(0) img').attr('src'); // Assuming image URL is in the third column
         var imagePublicId = row.find('img').attr('alt'); // Assuming image public ID is in the alt attribute

        // Populate form fields with extracted category data
        $('#editCategoryId').val(categoryId); // Set the category ID in a hidden input field
        $('#editTitle').val(title);
        $('#editDescription').val(description);
        $('#editImagePreview').attr('src', imageUrl);
        $('#editImagePreview').attr('alt', imagePublicId);
console.log(imagePublicId)
        // Open the edit modal
        $('#editCategoryModal').modal('show');
      });

      // Event listener for save changes button
      $('#saveChangesBtn').click(function () {
        // Get the category ID from the hidden input field
        var categoryId = $('#editCategoryId').val();

        // Logic to save changes to the server
        // You can use AJAX to send the form data to the backend, or submit the form directly
        // Example using AJAX:
        $.ajax({
          type: 'POST',
          url: '/admin/edit-categorylist/' + categoryId, // Include the category ID in the URL
          data: $('#editCategoryForm').serialize(), // Serialize the form data
          success: function (response) {
            // Handle the response from the server
            console.log(response);
          },
          error: function (error) {
            // Handle any errors
            console.error(error);
          }
        });

        // Close the modal after saving changes
        $('#editCategoryModal').modal('hide');
      });
    });
</script>

