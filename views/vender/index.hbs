  <h1>My Chart</h1>
  <div id="chartContainer" style="height: 370px; width: 80%;background-color: black; margin-left: 15%;"></div>

  <script>
    // Function to render the chart with the provided data
    function renderChart(chartData) {
      // Prepare dataPoints array for CanvasJS
      const dataPoints = chartData.map(entry => ({
        label: entry.label,
        y: entry.y
      }));

      const chart = new CanvasJS.Chart("chartContainer", {
        animationEnabled: true,
        title: {
          text: "Total Orders",
          color: "white",
        },
        data: [{
          type: "column",
          dataPoints: dataPoints,
        }],
        // Custom chart options
        backgroundColor: "black", // Change the background color here
        text: "white",
      });

      // Render the chart
      chart.render();
    }

    fetch('/vender/chart-data')
      .then(response => {
        if (!response.ok) {
          throw new Error('Network response was not ok');
        }
        return response.json();
      })
      .then(data => {
        const chartData = data.chartData;
        renderChart(chartData);

      })
      .catch(error => {
        console.error('Error fetching or rendering chart:', error);
      });
  </script>


