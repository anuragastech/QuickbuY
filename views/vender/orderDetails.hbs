    <script>
        $(document).ready(function() {
            var currentPage = 1;
            var rowsPerPage = 10; // Number of rows per page
            var orders = []; // This should be populated with your order data
            
            // Function to display orders for a specific page
            function displayOrders(page) {
                var startIndex = (page - 1) * rowsPerPage;
                var endIndex = startIndex + rowsPerPage;
                var paginatedOrders = orders.slice(startIndex, endIndex);

              

            }

            // Function to generate pagination buttons
            function generatePaginationButtons(totalPages) {
                $('#pagination').empty();
                for (var i = 1; i <= totalPages; i++) {
                    var button = $('<button>').text(i);
                    if (i === currentPage) {
                        button.addClass('active');
                    }
                    button.click(function() {
                        currentPage = parseInt($(this).text());
                        displayOrders(currentPage);
                        generatePaginationButtons(Math.ceil(orders.length / rowsPerPage));
                    });
                    $('#pagination').append(button);
                }
            }

            // Sample orders data (replace this with your actual data)
            var sampleOrders = [
                { product: 'Product 1', orderDate: '2024-01-01', totalAmount: 100, size: 'L', quantity: 2, price: 50, paymentStatus: 'paid', shippingStatus: 'shipped' },
                // Add more sample orders here
            ];

            orders = sampleOrders; // Assign your actual order data to the 'orders' variable

            displayOrders(currentPage);
            generatePaginationButtons(Math.ceil(orders.length / rowsPerPage));
        });
    </script>












