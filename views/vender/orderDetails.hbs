<script>
 function updateStatus(id, status) {
    // Update the dropdown button text with the selected status
    // You can also update the data in your backend using a fetch request
    fetch('/vender/updateShippingStatus', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json'
        },
        body: JSON.stringify({
            id: id,
            status: status
        })
    })
    .then(response => response.json())
     .then(data => {
        console.log(data);
        window.location.href = window.location.href;
    })
    
    .catch(error => console.error('There was a problem with the fetch operation:', error));
}


</script>
